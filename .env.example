# Arquivo de exemplo de configuração de ambiente
# Copie este arquivo para .env e preencha com seus dados

# Configurações do Spotify
SPOTIFY_CLIENT_ID=seu_client_id_aqui
SPOTIFY_CLIENT_SECRET=seu_client_secret_aqui
SPOTIFY_REDIRECT_URI=http://localhost:3000/callback

# Configuração da sessão
SESSION_SECRET=sua_chave_secreta_super_forte_aqui

# Configuração do banco de dados (opcional)
MONGODB_URI=mongodb://localhost:27017/spotify_wrapper

# Configuração do servidor (opcional)
PORT=3000
NODE_ENV=development
